{
  "name": "imagi-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:debug": "vite --host --debug",
    "build": "vue-tsc --noEmit && vite build",
