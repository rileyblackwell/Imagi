{
  "name": "imagi-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:debug": "vite --host --debug",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "start": "vite preview --host --port $PORT",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "clean": "rm -rf node_modules dist .vite",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@stripe/stripe-js": "^3.0.3",
    "@vee-validate/i18n": "^4.15.0",
    "@vee-validate/rules": "^4.15.0",
    "axios": "^1.6.7",
    "gsap": "^3.12.5",
    "pinia": "^2.1.7",
    "tailwindcss": "^3.4.1",
    "vee-validate": "^4.15.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "autoprefixer": "^10.4.17",
    "@eslint/eslint-config": "^0.1.6",
    "@eslint/config-array": "^0.1.3",
    "@eslint/object-schema": "^0.1.1",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "vite": "^5.0.12",
    "@types/node": "^20.11.0",
    "@vue/tsconfig": "^0.5.1",
    "typescript": "~5.3.3",
    "vue-tsc": "^1.8.27"
  }
}
